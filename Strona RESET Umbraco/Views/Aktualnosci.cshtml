@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Aktualnosci>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "Layout.cshtml";
	ViewBag.ActivePage = 1;
}
<div class="section service">
    <div class="container">
