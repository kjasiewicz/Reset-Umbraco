@inherits Umbraco.Web.Mvc.UmbracoTemplatePage

@{
    Layout = "Layout.cshtml";
    ViewBag.ActivePage = 2;
}
<link href="/css/magnific-popup.css" rel="stylesheet" />
<link href="/css/Gallery.css" rel="stylesheet" />

<section class="gallery">
    <div class="container popup-gallery">
        <div class="row zoom-gallery">
            <div class="col-lg-12 text-center">
                <h1 class="reset-font-large">lksdnkjd</h1>
                <br />
                <h3 class="reset-font-small">drglkndkjg</h3>
            </div>
            <div class="col-lg-3 col-md-4 col-xs-6 thumb">
                <a class="thumbnail" href="@Umbraco.TypedMedia(Umbraco.Field("galleryPhotosMini").ToString()).Url">
                    <img class="img-responsive" src="@Umbraco.TypedMedia(Umbraco.Field("galleryPhotos").ToString()).Url" alt="">
                </a>
            </div>
        </div>
    </div>
</section>

@section Scripts{
    <script type="text/javascript" src="~/Scripts/gallery/jquery.magnific-popup.js"></script>
    <script type="text/javascript" src="~/Scripts/gallery/customGallery.js"></script>

}