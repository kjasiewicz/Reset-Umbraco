<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<umbPackage>
  <files>
    <file>
      <guid>Nibble.Umb.ZipUpload.dll</guid>
      <orgPath>/bin</orgPath>
      <orgName>Nibble.Umb.ZipUpload.dll</orgName>
    </file>
    <file>
      <guid>zipUpload.config</guid>
      <orgPath>/config</orgPath>
      <orgName>zipUpload.config</orgName>
    </file>
    <file>
      <guid>zipupload.ascx</guid>
      <orgPath>/usercontrols</orgPath>
      <orgName>zipupload.ascx</orgName>
    </file>
  </files>
  <info>
    <package>
      <name>Zip Upload</name>
      <version>1.0</version>
      <license url="http://www.opensource.org/licenses/mit-license.php">MIT license</license>
      <url>http://www.nibble.be</url>
      <requirements>
        <major>3</major>
        <minor>0</minor>
        <patch>0</patch>
      </requirements>
    </package>
    <author>
      <name>Tim Geyssens</name>
      <website>http://www.nibble.be</website>
    </author>
    <readme><![CDATA[Small dashboardcontrol that allows to upload a zip file to the media section of umbraco. All files in the zip get created as media items.]]></readme>
  </info>
  <DocumentTypes />
  <Templates />
  <Stylesheets />
  <Macros />
  <DictionaryItems />
  <Languages />
  <DataTypes />
  <Actions>
    <Action runat="install" alias="addDashboardSection" dashboardAlias="MyDashboardSection">
      <section>
        <areas>
          <area>media</area>
        </areas>
        <tab caption="Zip Upload">
          <control>/usercontrols/zipupload.ascx</control>
        </tab>
      </section>
    </Action>
  </Actions>
</umbPackage>